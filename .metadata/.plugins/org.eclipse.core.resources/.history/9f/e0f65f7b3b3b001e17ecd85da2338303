		};
		Object result_data[] = new Object[country[0].length];
		/*
		 * country[0][0] count[1][0]
		 * country[0][1] count[1][1]
		 * country[0][2] count[1][2]
		 * country[0][3] count[1][3]
		 * */
		
	}
