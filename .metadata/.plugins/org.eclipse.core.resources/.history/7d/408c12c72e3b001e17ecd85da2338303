package array_test;

import java.util.*;

public class deletesoon {

	public static void main(String[] args) {
		deletesoon ds = new deletesoon();
		ds.test();
	}

	public void test() {
		/*
		 * 응용문제 8.
		 * 해당 데이터 배열에 있는 모든 값 중 짝수 값만 모두 더하여 결과값을
		 * 출력 하는 코드를 작성하시오.
		 * 1번 데이터 : 11 42 22 16
		 * 2번 데이터 : 7 33 10 29
		 * */
		int age [][] = {
				{11,42,22,16},
				{7,33,10,29}
		};
		
		for(int[] a : age) {
			System.out.println(Arrays.toString(a));
		}
	}
}
