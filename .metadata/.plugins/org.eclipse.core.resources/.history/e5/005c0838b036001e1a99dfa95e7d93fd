<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<input type="button" value="배열1" onclick="firstfunc()">
	<input type="button" value="배열2" onclick="secondfunc()">
	<input type="button" value="배열3" onclick="thirdfunc()">
	<input type="button" value="배열4" onclick="fourthfunc()">
	
	<div id="area">
		
	</div>
	
	<script>
		function firstfunc(){
			var data = {
				"myinfo1" : ["홍길동","25", "hong@nate.com"],
				"myinfo2" : ["김","21", "kim@nate.com"]
			}
			console.log(data);
		}
		
		function secondfunc(){
			var data = {
				"myinfo" : [
				{
					"id" : "hong",
					"name" : "홍길동",
					"age" : "25"
				},
				{
					"id" : "kim",
					"name" : "김기꾼길동",
					"age" : "98"
				}
				]
			};			
			console.log(data);
			var no = 0;
			var datalength = data.myinfo.length;
			console.log(datalength)

			while(no < datalength){
				var html = document.getElementById("area");
				html.innerHTML += `<ul>
					<li>아이디 : ${data.myinfo[no]["id"]}</li>
					<li>네임 : ${data.myinfo[no]["name"]}</li>
					<li>나이 : ${data.myinfo[no]["age"]}</li>
				</ul>`;	
				no++;
			}	
		}
		
		function thirdfunc(){
			var data = {
				myinfo : [
					["hong","25", "hong@nate.com"],
					["kim","30", "kim@nate.com"]
				]
			};
		}
	</script>
	
</body>
